<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ybveg.govx.system.mapper.ModuleMapper">
  <resultMap id="BaseResultMap" type="com.ybveg.govx.system.model.po.Module">
    <result column="id" jdbcType="VARCHAR" property="id"/>
    <result column="module_code" jdbcType="VARCHAR" property="moduleCode"/>
    <result column="clazz" jdbcType="VARCHAR" property="clazz"/>
    <result column="name" jdbcType="VARCHAR" property="name"/>
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
  </resultMap>

  <sql id="cToPForLfp">

  </sql>
  <select id="listForPage" parameterType="Integer" resultType="java.util.Map">
    SELECT * FROM sys_module
    ORDER BY create_time
    LIMIT 3, 2
  </select>

</mapper>