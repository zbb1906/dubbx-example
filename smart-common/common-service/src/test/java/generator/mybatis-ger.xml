<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--数据库驱动,最好不要有中文字符,不然会找不到-->


    <context id="DB2Tables"    targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库链接地址账号密码-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.7/govx_dev?characterEncoding\=utf-8" userId="gov" password="yuanben">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--todo 生成Model类存放位置-->
        <javaModelGenerator targetPackage="com.shundian.govx.api.po.biz" targetProject="D:\govx\db-core\src\test\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--todo 生成映射文件存放位置-->
        <sqlMapGenerator targetPackage="com.shundian.govx.mapper.biz" targetProject="D:\govx\db-core\src\test\java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--todo 生成DaoMapper类存放位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.shundian.govx.mapper.biz" targetProject="D:\govx\db-core\src\test\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!--todo 生成对应表及类名,需要记住的一点是逆向工程无法生成关联关系,只能生成单表操作-->
        <table tableName="check_report" domainObjectName="CheckReport"></table>
        <table tableName="check_report_item" domainObjectName="CheckReportItem"></table>

    </context>
</generatorConfiguration>